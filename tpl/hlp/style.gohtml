.pbg{
    position:absolute;
filter: blur(2px) opacity(0.24);
-webkit-filter:  blur(2px) opacity(0.24);
pointer-events:none;
}
header .pbg{
  bottom:-1.62vh;
  left:-1.62vh;
transform: scale(4) translate(2vh, 0) skew(38deg, 0deg);
}
footer .pbg{
  top:-1.62vh;
  right:-1.62vh;
transform: scale(4) translate(-2vh, 0) skew(-38deg, 0deg);
}


